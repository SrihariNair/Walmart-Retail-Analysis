    "-- ---------------------------- Generic ------------------------------\n",
    "-- --------------------------------------------------------------------\n",
    "-- How many unique cities does the data have?\n",
    "SELECT \n",
    "\tDISTINCT city\n",
    "FROM sales;\n",
    "\n",
    "-- In which city is each branch?\n",
    "SELECT \n",
    "\tDISTINCT city,\n",
    "    branch\n",
    "FROM sales;"
